@model coursework.Models.Clients

@{
    ViewBag.Title = "Просмотр клиента";
}

<h2>Детали</h2>

<div>
    <h4>Клиенты</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ФИО
        </dt>

        <dd>
            @Html.DisplayFor(model => model.FullName)
        </dd>

        <dt>
            Контактная информация
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ContactInfo)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.ClientID }) |
    @Html.ActionLink("Вернуться назад", "Index")
</p>
